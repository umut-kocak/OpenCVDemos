{
    "Settings": {
        "demo": {
			"style_image": "vangogh.jpg",
			"save_frames": true
        },
        "video": {
            "capture": {
				"source": "video.mp4",
                "threading": "SINGLE",
				"frame_filtering": {
					"method": "SKIP_FRAME",
					"skip_frame": {
						"action": "PICKUP",
						"period": 60
					}
				}
            },
			"output": {
				"file_name": "output_video.mp4",
				"fourcc": "mp4v"
			}			
        },
        "frame": {
            "missing_to_tolerate": -200,
            "processing": {
                "resize_capture_before_process": true,
                "width": 320,
                "height": 240
            }
        },
        "stats": {
            "show_help": true,
            "show_stats": true
        }
    }
}